{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "LearnJenkinsApp",
            "image": "286560211195.dkr.ecr.us-west-2.amazonaws.com/learnjenkinsapp:1.0.140",
            "portMappings": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::286560211195:role/ecsTaskExecutionRole"
}